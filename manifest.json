{
  "manifest_version": 2,
  "name": "demo",
  "version": "0.0.1",
  "description": "demo",
  "background": {
    "scripts": [
      "lib/background.js"
    ]
  },
  "permissions": [
    "notifications",
    "alarms",
    "contextMenus",
    "cookies",
    "activeTab",
    "desktopCapture",
    "tabs",
    "fontSettings",
    "identity",
    "management",
    "idle",
    "power",
    "system.cpu",
    "<all_urls>"
  ],
  "optional_permissions":[
    "downloads"
  ],
  "browser_action": {
    "default_title": "demo title",
    "default_popup": "pages/popup.html"
  },
  "devtools_page": "pages/devtools.html",
  "default_locale": "zh_CN",
  "omnibox": {
    "keyword": "ci"
  }
}
